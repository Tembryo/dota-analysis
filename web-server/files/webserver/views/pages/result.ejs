<%  layout('base-template'); 
    navigation_entries=[
    ];
-%>


<link href="/static/css/results.css" rel="stylesheet">

<!-- Wisdota Featurette -->
<div id ="results-container">
    <div class="container">
        <div class="row">
            <div class="col-md-12 text-center">
                <h1 class="results-page-header"> </h1>
            </div>
            <div class="col-md-1"></div>
            <div class="col-md-5 text-center">
                <div class="row results-box">
                    <div class="col-xs-10 col-xs-offset-1 text-center">
                        <img id="hero-img" src="/static/img/heroes/pictures/Earthshaker.png">
                    </div>
                </div>
                <div class="row"> 
                    <div class="col-xs-10 col-xs-offset-1 text-center">
                        <h2 class="results-header-small"></h2>
                    </div>
                </div>
                <div class="row barometer-container">
                    <div class="col-xs-12 text-center">
                        <table style="min-width:420px; margin: auto"><tr>
                            <td class="my-block">
                                    <img id="block1" src="/static/img/logos/block1space.png" height="70px" width="70px">
                                    <p id="pblock1" class="block-text">Noob</p>
                            </td>
                            <td class="my-block">
                                    <img id="block2" src="/static/img/logos/block2space.png" height="70px" width="70px">
                                    <p id="pblock2" class="block-text">Ok</p>
                            </td>
                            <td class="my-block">
                                    <img id="block3" src="/static/img/logos/block3space.png" height="70px" width="70px">
                                    <p id="pblock3" class="block-text">Nice</p>
                            </td>
                            <td class="my-block">
                                    <img id="block4" src="/static/img/logos/block4space.png" height="70px" width="70px">
                                    <p id="pblock4" class="block-text">Good</p>
                            </td>
                            <td class="my-block">
                                    <img id="block5" src="/static/img/logos/block5space.png" height="70px" width="70px">
                                    <p id="pblock5" class="block-text">Great</p>
                            </td> 
                            <td class="my-block">
                                    <img id="block6" src="/static/img/logos/block6space.png" height="70px" width="70px">
                                    <p id="pblock6" class="block-text">Godlike</p>
                            </td>
                        </tr></table>
                    </div>
                </div>
            </div>
            <div class="col-md-5 text-center">
                <div class="row top-row">
                    <div class="col-xs-4 col-xs-offset-2 text-center">
                        <a href="/scoreboard/<%=match_id%>" id="scoreboard-page" class="btn btn-success main-button">IMR Scoreboard</a>
                    </div>
                    <div class="col-xs-4 text-center">
                        <a href="/match/<%=match_id%>" id="match-page" class="btn btn-success main-button">Match Review</a>
                    </div>
                </div>

                <div class="row">
                    <div class="col-xs-4 text-center">
                        <!--Farming-->
                    </div>
                    <div class="col-xs-8 text-center progress progress-row">
                        <div id="skill-bar1" class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width:40%">

                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-xs-4 text-center">
                        <!--Fighting-->
                    </div>
                    <div class="col-xs-8 text-center progress progress-row">
                        <div id="skill-bar2" class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width:40%">

                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-xs-4 text-center">
                        <!--Movement-->
                    </div>
                    <div class="col-xs-8 text-center progress progress-row">
                        <div id="skill-bar3" class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width:40%">

                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-xs-4 text-center">
                        Interface Controls
                    </div>
                    <div class="col-xs-8 text-center progress progress-row">
                        <div id="skill-bar4" class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width:40%">

                        </div>
                    </div>
                </div>

            </div>
        </div>
        <div class="col-md-1"></div>
        </div>
    </div>
</div>

<div id="social-media-container">
    </div class="container">    
        <div class="row">
            <div class="col-lg-4 col-md-4 col-sm-2"></div>
            <div id="social-media" class="col-lg-4 col-md-4 col-sm-8 text-center">
                <h4>Share this rating with your friends:</h4>
                <div class="socialmedia_container center-block">

                    <div class="fb-send socialmediabutton" data-href="http://wisdota.com"></div>
                    <% tweet_text = "Rate your play on Wisdota! " %>
                    <a href="https://twitter.com/share" class="twitter-share-button"{count} data-text="<%=tweet_text%>" data-url="http://www.wisdota.com/" data-hashtags="wisdota,dota2">Tweet</a>
                    <script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+'://platform.twitter.com/widgets.js';fjs.parentNode.insertBefore(js,fjs);}}(document, 'script', 'twitter-wjs');</script>
                     <a href="mailto:?subject=Ready to turn pro?&body=Let Artificial Intelligence rate your Dota play! http://wisdota.com" class="mailLink socialmediabutton hidden-xs">
                     <img src="/static/img/email.svg" width="61px" height="20px">
                     </a>

                    <div class="fb-share-button" data-href="http://wisdota.com" data-layout="button_count"></div>

                </div>
                <!--<a href="index.html" id="rate-another-player-btn" class="btn btn-default main-button">Rate Another Player </a>-->
            </div>
            <div class="col-lg-4 col-md-4 col-sm-2"></div> 
        </div>

    </div>        
</div>


<script type="text/javascript">
    result_id = <%=result_id%>;
    match_id = <%=match_id%>;
    example = <%=example%>;
</script>
<script type="text/javascript" src="/static/js/results.js"></script>